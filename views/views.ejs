<link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=Playfair+Display" />
<link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=Varela+Round" />
<link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=Questrial" />
<% if (setting.data) {
    if (setting.data.faq_template_number) {
        switch(setting.data.faq_template_number) {
        case 1 : %>
            <%- include('./templates/one.ejs') %>
            <% if(messagesSetting.faq_messages_visible){%>
                <%- include('./templates/faq_question.ejs') %>
            <% }%>
        <% break;
        case 2 : %>
            <%- include('./templates/two.ejs') %>
            <% if(messagesSetting.faq_messages_visible){%>
                <%- include('./templates/faq_question.ejs') %>
            <%  }%>    
        <% break;
        case 3 : %>
            <%- include('./templates/three.ejs') %>
            <% if(messagesSetting.faq_messages_visible){%>
                <%- include('./templates/faq_question.ejs') %>
            <%  }%>   
        <% break;
        default: %>
        <%- include('./templates/one.ejs') %>
        <% break; }
    }else { %>
    <%- include('./templates/one.ejs') %>
    <% } %>
    <style>
        .send__faq-submit{
            color: <%- messagesSetting.text_color %>;
            background-color: <%- messagesSetting.theme_color %>;
            padding: 10px 16px;
            border-radius: 4px;
            border: none;
            font-size: 16px;
            cursor: pointer;
            line-height: 1.5;
            font-family: <%- messagesSetting.font_family %>;
        }

        .send__faq-header{
            border-bottom: 1px solid #ccc;
            display: flex;
            align-items: flex-start;
            justify-content: space-between;
            padding: 16px;
            font-weight: 500;
            font-size: 16px;
            background-color: <%- messagesSetting.theme_color %>;
            color: <%- messagesSetting.text_color %>;
            border-radius: 6px 0 6% 6%;
          }
        
        .send__faq-footer{
            display: flex;
            justify-content: space-between;
            padding: 16px;
            background-color: #f8f8f8;
            border-radius: 0 0 0 6px;
        }

        .send__faq-question, .send__faq-name, .send__faq-contact-more {
            margin-bottom: 16px;
        }

        .send__faq-component{
            display: flex;
            flex-direction: column;
            border: 1px solid #dee2e6; 
            border-radius: 6px; 
            background-color: #fff; 
            padding: 16px;
            box-shadow: 1px 1px 8px #ccc;
        }

        .send__faq-scroll::-webkit-scrollbar{
            width: 5px;
            background-color: #F5F5F5;
        }

        .send__faq-scroll::-webkit-scrollbar-thumb{
            background-color: gray;	
            border-radius: 5px 0 0 0;
        }

        .send__faq-icon:hover{
            border: 1px solid #ccc;
            border-radius: 4px;
          }
          .send__faq-text{
            transform: rotate(-90deg);
            white-space: nowrap;
          }
          .send__faq-hi{
            font-size: 30px;
            text-align: start;
            margin: 0;
          }
        
        .send__faq-height{
            max-height: 550px; 
            overflow-y: auto;
        }

        .send__faq-container{
            display: none;
            position: fixed;
            bottom: 100px;
            width: 400px;
            border-radius: 6px 0 0 6px;
            background-color: white;
            z-index: 9999999999;
            font-family: <%- messagesSetting.font_family %>;
            <% if (messagesSetting.aligment_faq_messages === 'left') { %>
                left: 20px;
            <% } %>
            <% if (messagesSetting.aligment_faq_messages === 'right') { %>
                right: 20px;
            <% } %>
        }
        @media only screen and (max-height: 650px) {
            .send__faq-container{
                bottom: 0;
            }
            .send__faq-height{
                max-height: calc(100vh - 30px); 
                overflow-y: auto;
            }
        }

        .custom-scroll::-webkit-scrollbar{
            width: 5px;
            background-color: #F5F5F5;
        }
          
        .custom-scroll::-webkit-scrollbar-thumb {
            background-color: gray;	
            border-radius: 5px;
        }

        .send__faq-textarea{
            padding: 8px;
            max-width: 100%;
            min-width: 100%;
            min-height: 120px;
            max-height: 200px;
            border: 1px solid #ccc;
            border-radius: 4px;
            outline: none;
            font-size: 16px;
            font-family: inherit;
            line-height: 1.5;
        }
        .send__faq-info{
            border: 1px solid #ccc;
            border-radius: 4px;
        }
        .send__faq-input{
            width: 100%;
            padding: 8px; 
            border-radius: 4px; 
            outline: none; 
            border: none; 
            font-size: 16px;
            font-family: inherit;
            line-height: 1.5;
        }
        .send__faq-icon{
            height: 40px;
            width: 40px;
            cursor: pointer;
            display: flex;
        }
        .send__faq{
            font-size: 16px;
            z-index: 999999999999;
        }
        @media only screen and (max-width: 500px) {
            .send__faq-container{
                display: none;
                border: 1px solid #ccc;
                border-radius: 6px;
                background-color: white;
                <% if (messagesSetting.aligment_faq_messages === 'left') { %>
                    left: 0;
                <% } %>
                <% if (messagesSetting.aligment_faq_messages === 'right') { %>
                    right: 0;
                <% } %>    
                overflow-y: auto;
                bottom: 0;
                border: 1px solid #ccc;
                border-radius: 6px;
                background-color: white;
            }
        }
        @media only screen and (max-width: 400px) {
            .send__faq-container{
                width: 100vw;
            }
        }
        .send__faq-body{
            padding: 16px 16px 0 16px;
            background-color: #f8f8f8;
        }
        .send__faq-button{
            display: flex;
            align-items:center;
            justify-content: center;
            width: 66px; 
            height: 66px;
            border-radius: 50%; 
            background-color: <%- messagesSetting.button_background_color %>; 
            cursor:pointer;
            position:fixed; 
            bottom: 20px; 
            <% if (messagesSetting.aligment_faq_messages === 'left') { %>
                left: 20px;
            <% } %>
            <% if (messagesSetting.aligment_faq_messages === 'right') { %>
                right: 20px;
            <% } %>
            z-index: 999999999999;
        }
        .send_faq-contact-link{
            padding: 8px; 
            border-radius: 4px;
            display: flex; 
            justify-content: center; 
            align-items: center;
        }
        .send__faq-label{
            margin-bottom: 4px;
            font-weight: 700;
            color: #5c6282;
        }
        <% if (setting.templateSetting) { %>
        .faq-category-title {
        <% if (setting.templateSetting.category_font_weight) { %>
            font-weight: <%=  setting.templateSetting.category_font_weight %>;
        <% } if (setting.templateSetting.category_font_color) { %>
            color: <%=  setting.templateSetting.category_font_color %>;
        <% } if (setting.templateSetting.category_font_size) { %>
            font-size: <%=  setting.templateSetting.category_font_size %>px;
        <% } if (setting.templateSetting.category_font_family) { %>
            font-family: <%=  setting.templateSetting.category_font_family %>;
        <% } if (setting.templateSetting.category_text_style) { %>
            text-transform: <%=  setting.templateSetting.category_text_style %>;
        <% } if (setting.templateSetting.category_text_align) { %>
            text-align: <%=  setting.templateSetting.category_text_align %>;
        <% } %>
        }
        .faq-question-title {
        <% if (setting.templateSetting.faq_font_weight) { %>
            font-weight: <%=  setting.templateSetting.faq_font_weight %>;
        <% } if (setting.templateSetting.faq_font_color) { %>
            color: <%=  setting.templateSetting.faq_font_color %> ;
        <% } if (setting.templateSetting.faq_bg_color) { %>
            background-color: <%=  setting.templateSetting.faq_bg_color %>;
        <% } if (setting.templateSetting.faq_font_size) { %>
            font-size: <%=  setting.templateSetting.faq_font_size %>px;
        <% } if (setting.templateSetting.faq_font_family) { %>
            font-family: <%=  setting.templateSetting.faq_font_family %>;
        <% } %>
        }
        <% if (setting.templateSetting.faq_hover_color) { %>
            .faq-question-title:hover {
                background-color: <%=  setting.templateSetting.faq_hover_color %>;
            }
        <% } %>
        .faq-answer-content {
        <% if (setting.templateSetting.answer_font_weight) { %>
            font-weight: <%=  setting.templateSetting.answer_font_weight %>;
        <% } if (setting.templateSetting.answer_font_color) { %>
            color: <%=  setting.templateSetting.answer_font_color %>;
        <% } if (setting.templateSetting.answer_bg_color) { %>
            background-color: <%=  setting.templateSetting.answer_bg_color %>;
        <% } if (setting.templateSetting.answer_font_size) { %>
            font-size: <%=  setting.templateSetting.answer_font_size %>px;
        <% } if (setting.templateSetting.answer_font_family) { %>
            font-family: <%=  setting.templateSetting.answer_font_family %>;
        <% } %>
        }
        <% } %>
    </style>
<% } %>