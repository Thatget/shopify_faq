<% if(setting.data.faq_page_schema) {%>
  <div class="root-faq-template" itemscope itemtype="https://schema.org/FAQPage">
 <% }  
else{ %>
  <div class="root-faq-template">
<% } %>
    <div class="faq-header-4745">
        <div>
            <% if(setting.data.page_title_content && setting.data.show_page_title) { %>
                <h1 class="faq-page-title"><%- setting.data.page_title_content %></h1>
            <% } %>
            <% if(setting.templateSetting.show_search_input) { %>
                <% if(setting.data.show_page_title && setting.data.page_title_content){%>
                <div style="width: 100%; background-color: rgb(245, 245, 245); text-align: center">
                  <div class="faq-page-header">
                    <div class="yanet-select-category" style="flex: 1; display: flex; flex-direction: column;">
                      <label for="categories" style="text-align: start; font-weight: 700; font-size: 16px; padding: 0; margin: 0">Select category</label>
                      <select class="faq-dropdown-category" name="categories" id="categories" onChange="changeCategory('<%= setting.templateSetting.category_bar_color %>', '<%= setting.templateSetting.category_bar_background %>')">
                        <% for(var i = 0; i < faqs.categories.length; i++) { %>
                            <option value="<%- i %>"><%- faqs.categories[i].title %></option>
                        <% } %> 
                      </select>                    
                    </div>
                      <div class="faq-search" style="box-shadow: none; display: flex; flex-direction: column;">
                        <label for="search-text" style="text-align: start; font-weight: 700; font-size: 16px; padding: 0; margin: 0">Search</label>
                        <% if(setting.templateSetting.search_input_style == 1) { %>
                            <input class="faq-search-input faq-search-input-style1" style="color: <%= setting.templateSetting.placeholder_color %>" id="search-text" onkeyup="simpleSearch()" type="text" <% if(setting.data.search_placehoder) {%> placeholder="<%= setting.data.search_placehoder %>" <%}%> name="search">
                        <% } %>   
                        <% if(setting.templateSetting.search_input_style == 2) { %>
                            <div style="position:relative; border-bottom: 1px solid rgb(187, 188, 188); height: 40px; margin: 15px 0px 12px;">
                                <input class="faq-search-input faq-search-input-style2" style="color: <%= setting.templateSetting.placeholder_color %>" id="search-text" onkeyup="simpleSearch()" type="text" <% if(setting.data.search_placehoder) {%> placeholder="<%= setting.data.search_placehoder %>" <%}%> name="search">
                                <svg id="micro-scope" style="position: absolute; right: 16px; top: 18%; margin-left: 18px" height="18" width="18" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512"><path d="M416 208c0 45.9-14.9 88.3-40 122.7L502.6 457.4c12.5 12.5 12.5 32.8 0 45.3s-32.8 12.5-45.3 0L330.7 376c-34.4 25.2-76.8 40-122.7 40C93.1 416 0 322.9 0 208S93.1 0 208 0S416 93.1 416 208zM208 352c79.5 0 144-64.5 144-144s-64.5-144-144-144S64 128.5 64 208s64.5 144 144 144z"/></svg>
                            </div>
                        <% } %>   
                    </div>
                  </div>
                </div>
                <%}
                else{%>
                    <div class="faq-search-no-title">
                        <% if(setting.templateSetting.search_input_style == 1) { %>
                            <input class="faq-search-input faq-search-input-style1" style="color: <%= setting.templateSetting.placeholder_color %>" id="search-text" onkeyup="simpleSearch()" type="text" <% if(setting.data.search_placehoder) {%> placeholder="<%= setting.data.search_placehoder %>" <%}%> name="search">
                        <% } %>
                        <% if(setting.templateSetting.search_input_style == 2) { %>
                            <div style="position:relative; border-bottom: 1px solid rgb(187, 188, 188); height: 40px">
                                <svg id="micro-scope" style="position: absolute; right: 16px; top: 18%; margin-left: 18px" height="18" width="18" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512"><path d="M416 208c0 45.9-14.9 88.3-40 122.7L502.6 457.4c12.5 12.5 12.5 32.8 0 45.3s-32.8 12.5-45.3 0L330.7 376c-34.4 25.2-76.8 40-122.7 40C93.1 416 0 322.9 0 208S93.1 0 208 0S416 93.1 416 208zM208 352c79.5 0 144-64.5 144-144s-64.5-144-144-144S64 128.5 64 208s64.5 144 144 144z"/></svg>
                                <input class="faq-search-input faq-search-input-style2" style="color: <%= setting.templateSetting.placeholder_color %>" id="search-text" onkeyup="simpleSearch()" type="text" <% if(setting.data.search_placehoder) {%> placeholder="<%= setting.data.search_placehoder %>" <%}%> name="search">                                
                            </div>
                        <% } %>
                    </div>
                <%} %>
            <% } %>
        </div>
        <div>
            <% if(setting.data.show_intro_text && setting.data.intro_text_content) {%>
                <div class="faq-intro-text">
                    <span><%- setting.data.intro_text_content %></span>
                </div>
            <% } %>    
        </div>
    </div>
    <div class="faq-main" id="faq-main">
        <% if(!setting.data.show_page_construction){%>
            <% if(faqs && !setting.data.dont_category_faq){ %>
                <div class="faq-main-container">
                    <div class="faq-category-container">
                        <% if(faqs.categories.length){%>
                              <div class="faq-category-menu">
                                  <div class="yanet-faq-bar" style="box-shadow: rgba(0, 0, 0, 0.15) 0px 2px 13px 0px;">
                                    <div class="faq-main-content" style="padding: 0; margin: 0" itemscope itemprop="mainEntity" itemtype="https://schema.org/Question">
                                        <div style="font-size: 16px; font-weight: 900; line-height: 20px; display: flex">
                                            <span class="listed-faq" style="padding: 20px 38px; border-top: 1px solid rgb(235, 235, 235); font-size: 16px" value itemprop="name">ARTICLES IN THIS CATEGORY</span>
                                        </div>
                                    </div>
                                    <% for(var i=0; i < faqs.categories.length; i++) { %>
                                      <div id="faq-list<%- i %>">
                                          <% for(var j=0; j < faqs.faq.length; j++) { %>
                                              <% if(faqs.faq[j].category_identify === faqs.categories[i].identify) {%>
                                                <% if(setting.data.faq_page_schema) {%>
                                                    <div class="faq-question-title yanet-faq-question" id="faq-id-<%- j %>" onclick="showHideContent(<%- j %>, -1)">
                                                        <span class="listed-faq" id="faq-bar<%- j %>" style="padding: 20px 38px;line-height: 20px; border-top: 1px solid rgb(235, 235, 235);" value itemprop="name"><%= faqs.faq[j].title %></span>
                                                    </div>
                                                <% }
                                                else{ %>
                                                    <div class="faq-question-title yanet-faq-question" id="faq-id-<%- j %>" onclick="showHideContent(<%- j %>, -1)">
                                                        <span class="listed-faq" id="faq-bar<%- j %>" style="padding: 20px 38px;line-height: 20px; border-top: 1px solid rgb(235, 235, 235);"><%= faqs.faq[j].title %></span>
                                                    </div>
                                                <% } %>
                                              <% } %>
                                          <%}%>
                                      </div>
                                    <% } %>                             
                                  </div>
                                  <div class="yanet-faq-select">
                                    <div class="faq-main-content" style="padding: 0; margin: 0" itemscope itemprop="mainEntity" itemtype="https://schema.org/Question">
                                        <div style="font-size: 16px; font-weight: 900; line-height: 20px; display: flex">
                                            <span style="font-size: 16px" value itemprop="name">ARTICLES IN THIS CATEGORY</span>
                                        </div>
                                    </div>
                                      <% for(var i=0; i < faqs.categories.length; i++) { %>
                                        <div id="faq-list-select<%- i %>" style="display: none">
                                          <select class="faq-dropdown-faq" name="categories" onChange="showHideContent(-1, <%- i %>)">
                                            <% for(var j = 0; j < faqs.faq.length; j++) { %>
                                              <% if(faqs.faq[j].category_identify === faqs.categories[i].identify) {%>
                                                  <option value="<%- j %>"><%- faqs.faq[j].title %></option>
                                              <% } %>
                                            <% } %> 
                                          </select>                    
                                        </div>
                                      <% } %>
                                  </div>
                              </div>
                            <div class="faq-container">
                                <div class="category-faq-container">
                                    <% for(var i=0; i < faqs.categories.length; i++) { %>
                                        <div id="category<%- i %>">
                                            <% for(var j=0; j < faqs.faq.length; j++) { %>
                                                <% if(faqs.faq[j].category_identify === faqs.categories[i].identify) {%>
                                                  <% if(setting.data.faq_page_schema) {%>
                                                    <div class="yanet-faq-container" id="yanet-faq-container-<%= j %>">
                                                      <div class="faq-main-content" style="padding: 0" itemscope itemprop="mainEntity" itemtype="https://schema.org/Question">
                                                          <div class="faq-question-title" style="margin: 16px 0 9px; font-size: 24px; text-transform: uppercase; font-weight: 700;">
                                                              <span class="listed-faq" value itemprop="name"><%= faqs.faq[j].title %></span>
                                                          </div>
                                                          <div id="faq-answer-content<%- j %>" class="faq-answer-content" itemscope itemprop="acceptedAnswer" itemtype="https://schema.org/Answer">
                                                            <div itemprop="text">
                                                              <%- faqs.faq[j].content %>
                                                            </div>
                                                          </div>
                                                      </div>                                              
                                                    </div>
                                                  <% } 
                                                  else{ %>
                                                    <div class="yanet-faq-container" id="yanet-faq-container-<%= j %>">
                                                      <div class="faq-main-content" style="padding: 0">
                                                          <div class="faq-question-title" style="margin: 16px 0 9px; font-size: 24px; text-transform: uppercase; font-weight: 700;">
                                                              <span class="listed-faq"><%= faqs.faq[j].title %></span>
                                                          </div>
                                                          <div id="faq-answer-content<%- j %>" class="faq-answer-content">
                                                            <div>
                                                              <%- faqs.faq[j].content %>  
                                                            </div>
                                                          </div>
                                                      </div>
                                                    </div>
                                                  <% } %>
                                                <% } %>
                                            <%}%>
                                        </div>
                                    <% } %>
                                </div>
                            </div>
                        <% } %>
                    </div>
                </div>
            <% }
            else{ %>
                <div class="faq-dont-category">
                    <div style="display: flex; flex-direction: column">
                        <% for(var j=0; j < faqs.faq.length; j++) { %>
                          <% if(setting.data.faq_page_schema) {%>
                            <div class="faq-main-content" itemscope itemprop="mainEntity" itemtype="https://schema.org/Question">
                                <div class="faq-question-title" onclick="showHideContent(this)">
                                  <span class="listed-faq" itemprop="name"><%= faqs.faq[j].title %></span>
                                </div>
                                <div class="faq-answer-content" itemscope itemprop="acceptedAnswer" itemtype="https://schema.org/Answer">
                                  <div itemprop="text">
                                    <%- faqs.faq[j].content %>
                                  </div>
                                </div>
                            </div>                          
                          <% } 
                          else{ %>
                            <div class="faq-main-content">
                                <div class="faq-question-title" onclick="showHideContent(this)">
                                  <span class="listed-faq"><%= faqs.faq[j].title %></span>14\+8
                                </div>
                                <div class="faq-answer-content">
                                  <div>
                                    <%- faqs.faq[j].content %>
                                  </div>
                                </div>
                            </div>     
                          <% } %>
                        <%}%>
                    </div>
                </div>
            <% } %>
            <div class="category-faq-search">
                <div style="display: flex; flex-direction: column">
                    <% for(var i=0; i < faqs.categories.length; i++) { %>
                        <div>
                            <% for(var j=0; j < faqs.faq.length; j++) { %>
                                <% if(faqs.faq[j].category_identify === faqs.categories[i].identify) {%>
                                  <% if(setting.data.faq_page_schema) {%>
                                    <div class="faq-main-content" itemscope itemprop="mainEntity" itemtype="https://schema.org/Question">
                                        <div class="faq-question-title" onclick="showHideContent(this)">
                                            <span class="listed-faq faq-question-search" itemprop="name"><%= faqs.faq[j].title %></span>
                                        </div>
                                        <div class="faq-answer-content faq-answer-search" itemscope itemprop="acceptedAnswer" itemtype="https://schema.org/Answer">
                                          <div itemprop="text">
                                            <%- faqs.faq[j].content %>
                                          </div>
                                        </div>
                                    </div>
                                  <% } 
                                  else{ %>
                                    <div class="faq-main-content">
                                        <div class="faq-question-title" onclick="showHideContent(this)">
                                            <span class="listed-faq faq-question-search"><%= faqs.faq[j].title %></span>
                                        </div>
                                        <div class="faq-answer-content faq-answer-search">
                                          <div>
                                            <%- faqs.faq[j].content %>
                                          </div>
                                        </div>
                                    </div>
                                  <% } %>
                                <% } %>
                            <%}%>
                        </div>
                    <% } %>
                </div>
            </div>
        <% }
        else{ %>
            <div class="page-under-contruction">
                <%- setting.data.page_under_contruction %>
            </div>
        <% } %>
        <div id="not-found-text">
            <% if(setting.data.search_not_found) {%>
                <%- setting.data.search_not_found %>
            <% } %>
        </div>
        <% if(setting.data.show_footer_text) {%>
            <div class="faq-footer-text">
                <% if(setting.data.footer_text_content) {%>
                    <%- setting.data.footer_text_content %>
                <% } %>
            </div>
        <% } %>
        <% if(setting.data.yanet_logo_visible) {%>
            <div class="" style="text-align: center; padding-bottom: 10px">
                <a href="https://apps.shopify.com/yanet-professional-faq-page" class="yanet-logo" target="_blank">Powered by Yanet</a>
            </div>
        <% } %>
        <% if(setting.templateSetting.btn_top_visible) {%>
            <div class="btn-back-to-top" onclick="topFunction()" id="back-to-top" title="Go to top">
                <svg fill="#ffffff" height="14" width="14" viewBox="0 0 448 512"><path d="M246.6 41.4c-12.5-12.5-32.8-12.5-45.3 0l-160 160c-12.5 12.5-12.5 32.8 0 45.3s32.8 12.5 45.3 0L224 109.3 361.4 246.6c12.5 12.5 32.8 12.5 45.3 0s12.5-32.8 0-45.3l-160-160zm160 352l-160-160c-12.5-12.5-32.8-12.5-45.3 0l-160 160c-12.5 12.5-12.5 32.8 0 45.3s32.8 12.5 45.3 0L224 301.3 361.4 438.6c12.5 12.5 32.8 12.5 45.3 0s12.5-32.8 0-45.3z"/></svg>
            </div>
        <% } %>
    </div>
  </div>
  <script>
    for(let i = 0; i < <%= faqs.categories.length %>; i++){
      document.getElementById(`category0`).style.display = 'block'
      document.getElementById(`category0`).children[0].style.display = 'flex'
      if(i == 0){
        var listFaqCurrent = document.getElementById(`faq-list${i}`)
        listFaqCurrent.style.display = 'block'
        listFaqCurrent.children[0].style.backgroundColor = '<%= setting.templateSetting.category_bar_background %>'
        listFaqCurrent.children[0].style.color = '#fff'
        listFaqCurrent.children[0].style.fontWeight = 'bold'
      }
      else{
        document.getElementById(`faq-list${i}`).style.display = 'none'
        document.getElementById(`category${i}`).style.display = 'none'
      }
    }
    if(document.getElementById('faq-list-select0')){
        document.getElementById('faq-list-select0').style.display = 'block'
      }
    if(document.getElementById("back-to-top")){
      let mybutton = document.getElementById("back-to-top");
      window.onscroll = function() {scrollFunction()};
      function scrollFunction() {
          if (document.body.scrollTop > 600 || document.documentElement.scrollTop > 600) {
              mybutton.style.display = "flex";
          } 
          else {
              mybutton.style.display = "none";
          }
      }
      function topFunction() {
          document.body.scrollIntoView({  
              behavior: "smooth"
          });    
      }
    }
    var categoryName = []
    function changeCategory(category_bar_color, category_bar_background){
      var id = document.getElementById("categories").value;
      
      if(!categoryName.some(item => {
          return item === id
      })){            
          categoryName.push(id)
      }
      var categoryShow = categoryName.filter(item => {
          return item === id
      })
      var categoryHide = categoryName.filter(item => {
          return item !== id
      })
      for(let i = 0; i < <%= faqs.categories.length %>; i++){
        if(i == id){
          if(document.getElementById(`category${i}`)){
            document.getElementById(`category${i}`).style.display = 'block'
            document.getElementById(`category${i}`).children[0].style.display = 'flex'
          }
        }
        else{
          if(document.getElementById(`category${i}`)){
            document.getElementById(`category${i}`).style.display = 'none'          
          }
        }
      }
      if(document.getElementById('faq-list0')){
        document.getElementById('faq-list0').style.display = 'none'
      }
      if(document.getElementById('faq-list-select0')){
        document.getElementById('faq-list-select0').style.display = 'none'
      }
      for(let i = 0; i < categoryName.length; i++){
        if(categoryName[i] == id){
          if(document.getElementById(`faq-list-select${id}`)){
            document.getElementById(`faq-list-select${id}`).style.display = 'block'
          }
          if(document.getElementById(`faq-list${id}`)){
            var listFaqCurrent = document.getElementById(`faq-list${id}`)
            listFaqCurrent.style.display = 'block'
            listFaqCurrent.children[0].style.backgroundColor = `${category_bar_background}`
            listFaqCurrent.children[0].style.color = '#fff'
            listFaqCurrent.children[0].style.fontWeight = 'bold'
          }
        }
        else{
          if(document.getElementById(`faq-list${id}`)){
            document.getElementById(`faq-list${categoryName[i]}`).style.display = 'none'
          }
          if(document.getElementById(`faq-list-select${categoryName[i]}`)){
            document.getElementById(`faq-list-select${categoryName[i]}`).style.display = 'none'
          }
        }
      }
    }

    function showHideContent(index, value) {
      let selected = 0
      if(index != -1){
        console.log(index)
        selected = index
      }
      else{
        selected = document.getElementsByClassName('faq-dropdown-faq')[value].value
      }
      for(let i = 0; i < <%= faqs.faq.length %>; i++){
        if(i == selected){
          if(document.getElementById(`faq-id-${i}`)){
            document.getElementById(`faq-id-${i}`).style.backgroundColor = '<%= setting.templateSetting.category_bar_background %>'
            document.getElementById(`faq-id-${i}`).style.color = '#fff'
            document.getElementById(`faq-id-${i}`).style.fontWeight = 'bold'
          }
          if(document.getElementById(`yanet-faq-container-${i}`)){
            document.getElementById(`yanet-faq-container-${i}`).style.display = 'flex'
          }
        }
        else{
          if(document.getElementById(`faq-id-${i}`)){
            document.getElementById(`faq-id-${i}`).style.backgroundColor = '#fff'
            document.getElementById(`faq-id-${i}`).style.color = '<%= setting.templateSetting.category_bar_color %>'
            document.getElementById(`faq-id-${i}`).style.fontWeight = '400'
          }
          if(document.getElementById(`yanet-faq-container-${i}`)){
            document.getElementById(`yanet-faq-container-${i}`).style.display = 'none'
          }
        }
      }    
    }

    function simpleSearch() {
        document.getElementsByClassName('faq-main-container')[0].style.padding = '0'
        var input = document.getElementById("search-text");
        var filter = input.value.toLowerCase();
        var filterByQuestion = document.getElementsByClassName('faq-question-search');
        var filterByAnswer = document.getElementsByClassName('faq-answer-search');
        var nodeCategory = document.getElementsByClassName('category-faq-search');
        if(<%=  setting.data.dont_category_faq%> == false){
            if(document.getElementById('search-text').value === ''){
                document.getElementsByClassName("faq-main")[0].style.alignItems = "unset"
                document.getElementsByClassName("category-faq-container")[0].style.display = "block"
                document.getElementsByClassName('faq-category-menu')[0].style.display = "block"
                // document.getElementsByClassName('faq-container')[0].style.width = "70%"
                nodeCategory[0].style.display = "none"
                for(i = 0; i < document.getElementsByClassName("faq-category-search").length; i++) {
                    document.getElementsByClassName("faq-category-search")[i].style.display = "block"
                }
            }
            else{
                document.getElementsByClassName("faq-main")[0].style.alignItems = "center"
                document.getElementsByClassName("category-faq-container")[0].style.display = "none"
                document.getElementsByClassName('faq-category-menu')[0].style.display = "none"
                // document.getElementsByClassName('faq-container')[0].style.width = "100%"
                nodeCategory[0].style.display = "flex"
                for(i = 0; i < document.getElementsByClassName("faq-category-search").length; i++) {
                    document.getElementsByClassName("faq-category-search")[i].style.display = "none"
                }
            }
            var checkexist = true;
            for (i = 0; i < filterByQuestion.length; i++) {
                if (filterByQuestion[i].innerText.toLowerCase().includes(filter)) {
                    filterByQuestion[i].parentNode.parentNode.style.display = "block";
                    checkexist = false;
                } 
                else {
                    for(j = 0; j < filterByAnswer.length; j++){
                        if(filterByAnswer[j].innerText.toLowerCase().includes(filter)){
                            filterByQuestion[j].parentNode.parentNode.style.display = "block";
                            checkexist = false;        
                        }
                        else{
                            filterByQuestion[i].parentNode.parentNode.style.display = "none";
                        }
                    }
                }
            }
            if (checkexist) {
                document.getElementById("not-found-text").style.display = "block";
                nodeCategory[0].style.display = "none";
            } else {
                document.getElementById("not-found-text").style.display = "none";
            }
        }
        else{
            if(document.getElementById('search-text').value == ''){
                document.getElementsByClassName('faq-dont-category')[0].style.display = "flex"
  
                nodeCategory[0].style.display = "none"
                for(i = 0; i < document.getElementsByClassName("faq-category-search").length; i++) {
                    document.getElementsByClassName("faq-category-search")[i].style.display = "block"
                }
            }
            else{
                document.getElementsByClassName('faq-dont-category')[0].style.display = "none"
  
                nodeCategory[0].style.display = "flex"
                nodeCategory[0].style.justifyContent = "center"
                for(i = 0; i < document.getElementsByClassName("faq-category-search").length; i++) {
                    document.getElementsByClassName("faq-category-search")[i].style.display = "none"
                }
            }
            var checkexist = true;
            for (i = 0; i < filterByQuestion.length; i++) {
                if (filterByQuestion[i].innerText.toLowerCase().includes(filter)) {
                    filterByQuestion[i].parentNode.parentNode.style.display = "flex";
                    checkexist = false;
                } 
                else {
                    for(j = 0; j < filterByAnswer.length; j++){
                        if(filterByAnswer[j].innerText.toLowerCase().includes(filter)){
                            filterByQuestion[j].parentNode.parentNode.style.display = "flex";
                            checkexist = false;        
                        }
                        else{
                            filterByQuestion[i].parentNode.parentNode.style.display = "none";
                        }
                    }
                }
            }
            if (checkexist) {
                document.getElementById("not-found-text").style.display = "block";
                nodeCategory[0].style.display = "none";
            } else {
                document.getElementById("not-found-text").style.display = "none";
            }        
        }
    }
