<script>
  let be_link = 'https://backend-faq.yanet.io'
  // let be_link = 'https://3eea-2001-ee0-8206-576f-15e6-fcd1-aabf-b6c8.ngrok-free.app'

  if(document.getElementById("back-to-top")){
    let mybutton = document.getElementById("back-to-top");
    window.onscroll = function() {scrollFunction()};
    function scrollFunction() {
        if (document.body.scrollTop > 600 || document.documentElement.scrollTop > 600) {
            mybutton.style.display = "flex";
        } 
        else {
            mybutton.style.display = "none";
        }
    }
    function topFunction() {
        document.body.scrollIntoView({  
            behavior: "smooth"
        }); 
    }
  }
    function showHideContent(el) {
        var content = el.nextElementSibling;
        var svgRight = el.getElementsByClassName('svg-right')
        var svgDown = el.getElementsByClassName('svg-down')
        if (content.style.display === "block") {
            content.style.display = "none";
            svgRight[0].style.transform = "rotate(0deg)";
            svgRight[0].style.transition  = "all .2s";
        } 
        else {
          // if(use_analytics_faq === true){
          //   let link = `${be_link}/api/shop/faq/update_analytics/${faq_id}`
          //   fetch(link)
          //   .then(response => response.json())
          // }
          content.style.display = "block";
          svgRight[0].style.transform = "rotate(90deg)";
          svgRight[0].style.transition  = "all .2s";
        }
    }

    function showMoreFaqs(el, number, number_faq_show){
      if(<%= !setting.data.dont_category_faq %>){
        let categoryFaqContainer = document.getElementsByClassName('category-faq-container')[number]
        for(let j = 0; j < categoryFaqContainer.getElementsByClassName('faq-main-content').length; j++){
          if(j >= number_faq_show){
            categoryFaqContainer.getElementsByClassName('faq-main-content')[j].style.display = "block"
          }
        }
        document.getElementsByClassName('btn-show-faq')[number].style.display = "none"
      }
      else{
        for(let j = 0; j < <%= faqs.faq.length %>; j++){
          if(j >= number_faq_show){
            document.getElementsByClassName('faq-main-content')[j].style.display = "block"
          }
        }
        document.getElementsByClassName('btn-show-faq')[number].style.display = "none"
      }
    }

    let page = 1
    async function loadMoreFaqs(){
      page += 1
      if(page){
        let link = `${be_link}/faq-proxy?shop=<%= shop %>&locale=<%= locale %>&plan=<%= plan %>&page=${page}`
        await fetch(link)
          .then(res => res.text())
          .then(result => {
            var faq = document.createElement('p'); // is a node
            faq.innerHTML = result;
            document.getElementsByClassName('faq-yanet-main')[0].appendChild(faq)
          })
        let linkCategory = `${be_link}/category-proxy?shop=<%= shop %>&locale=<%= locale %>&plan=<%= plan %>&page=${page}`
        await fetch(linkCategory)
          .then(res => res.text())
          .then(result => {
            document.getElementsByClassName('category-yanet-main')[0].innerHTML = result
          })
        if(<%= countFaqs %> <= page*100){
          document.getElementsByClassName('show-more-faq-button')[0].style.display = 'none !important'
        }
      }
    }

    function hideFaq(){
      if(<%= !setting.data.dont_category_faq %>){
        for(let i = 0; i < <%= faqs.categories.length %>; i++){
          let categoryFaqContainer = document.getElementsByClassName('category-faq-container')[i]
          for(let j = 0; j < categoryFaqContainer.getElementsByClassName('faq-main-content').length; j++){
            if(j >= 6){
              categoryFaqContainer.getElementsByClassName('faq-main-content')[j].style.display = "none"
            }
          }
          if(categoryFaqContainer.getElementsByClassName('faq-main-content').length > 6){
            document.getElementsByClassName('btn-show-faq')[i].style.display = "block"
            let numberFaqHide = (categoryFaqContainer.getElementsByClassName('faq-main-content').length - 6)
            document.getElementsByClassName('btn-show-faq')[i].innerText = `See more (${numberFaqHide} Faqs)`
          }
          else{
            document.getElementsByClassName('btn-show-faq')[i].style.display = "none"
          }
        }
      }
      else{
        for(let j = 0; j < <%= faqs.faq.length %>; j++){
          if(j >= 6){
            document.getElementsByClassName('faq-main-content')[j].style.display = "none"
          }
        }
        if(document.getElementsByClassName('faq-main-content').length > 6){
          document.getElementsByClassName('btn-show-faq')[0].style.display = "block"
          let numberFaqHide = (document.getElementsByClassName('faq-main-content').length - 6)
          document.getElementsByClassName('btn-show-faq')[0].innerText = `See more (${numberFaqHide} Faqs)`
        }
        else{
          document.getElementsByClassName('btn-show-faq')[0].style.display = "none"
        }      
      }
    }
    if(<%= setting.data.id %> == 4625){
      hideFaq()
    }

    function evaluateFaq(type, id, number){
      document.getElementById(`yanet-faq-evaluate-${id}`).style.display = "none"
      let link = `${be_link}/api/shop/faq/analytics-faq/${id}?type=${type}`
      fetch(link)
      .then(response => response.json())
      if(type === 'like'){
        document.getElementById(`liked-${id}`).style.display = "flex"
        document.getElementById(`faq-${id}-like`).innerText = number + 1
      }
      if(type === 'dislike'){
        document.getElementById(`disliked-${id}`).style.display = "flex"
        document.getElementById(`faq-${id}-dislike`).innerText = number + 1
      }
    }

    function simpleSearch() {
      var input = document.getElementById("search-text");
      var filter = input.value.toLowerCase();
      var filterByQuestion = document.getElementsByClassName('faq-question-search');
      var filterByAnswer = document.getElementsByClassName('faq-answer-search');
      var nodeCategory = document.getElementsByClassName('category-faq-container');
      var categoryBar = document.getElementsByClassName('category-bar')
      if(categoryBar.length > 0){
          if(document.getElementById('search-text').value === ''){
            document.getElementsByClassName('category-bar')[0].style.display = "flex"
            for(i = 0; i < nodeCategory.length; i++){
                document.getElementsByClassName("faq-category-width")[i].style.display = "block"
            }
            if(<%= setting.data.id %> == 4625){
              for(let i = 0; i < <%= faqs.categories.length %>; i++){
                document.getElementsByClassName('btn-show-faq')[i].style.display = "block"
              }
            }
          }
          else{
            document.getElementsByClassName('category-bar')[0].style.display = "none"
            for(i = 0; i < nodeCategory.length; i++){
                document.getElementsByClassName("faq-category-width")[i].style.display = "none"
            }
            if(<%= setting.data.id %> == 4625){
              for(let i = 0; i < <%= faqs.categories.length %>; i++){
                document.getElementsByClassName('btn-show-faq')[i].style.display = "none"
              }
            }
          }
      }
      var checkexist = true;
      for (i = 0; i < filterByQuestion.length; i++) {
          if (filterByQuestion[i].innerText.toLowerCase().includes(filter)) {
              filterByQuestion[i].parentNode.parentNode.style.display = "block";
              checkexist = false;
          }
          else {
              for(j = 0; j < filterByAnswer.length; j++){
                  if(filterByAnswer[j].innerText.toLowerCase().includes(filter)){
                      filterByQuestion[j].parentNode.parentNode.style.display = "block";
                      checkexist = false;        
                  }
                  else{
                      filterByQuestion[i].parentNode.parentNode.style.display = "none";
                  }
              }
          }
      }
      for (j = 0; j < nodeCategory.length; j++) {
          let subFaq = nodeCategory[j].getElementsByClassName('faq-main-content');
          let checkDisplay = false;
          for (k = 0; k < subFaq.length; k++) {
              if(subFaq[k].style.display == "block") {
                  checkDisplay = true;
                  break;
              }
          }
          if (checkDisplay) {
              nodeCategory[j].style.display = "block";
          } else nodeCategory[j].style.display = "none";
      }
      if (checkexist) {
          document.getElementById("not-found-text").style.display = "block";
      } else {
          document.getElementById("not-found-text").style.display = "none";
      }
      if(<%= setting.data.id %> == 4625){
        if(document.getElementById('search-text').value === ''){
          hideFaq()
        }
        if(categoryBar.length <= 0){
          if(document.getElementById('search-text').value === ''){
            document.getElementsByClassName('btn-show-faq')[0].style.display = "block"
          }
          else{
            document.getElementsByClassName('btn-show-faq')[0].style.display = "none"
          }
        }
      }
    }

</script>
