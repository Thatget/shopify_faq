                                    <div class="category-bar">
                                        <% for(var i=0; i < faqs.categories.length; i++) { %>
                                            <a href="#<%- faqs.categories[i].title.replace(/ /g, '-') %>" class="faq-category-title faq-category-link" style="color: <%=  setting.templateSetting.category_bar_color %>"><%- faqs.categories[i].title %></a>
                                        <% } %>
                                    </div>
                                  </div>
                                <% } %>
                                <% if(setting.templateSetting.category_bar_number === 2){%>
