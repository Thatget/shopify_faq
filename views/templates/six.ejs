                    <div class="faq-search" style="box-shadow: none;">
                        <% if(setting.templateSetting.search_input_style == 1) { %>
                            <input class="faq-search-input faq-search-input-style1" style="color: <%= setting.templateSetting.placeholder_color %>" id="search-text" onkeyup="simpleSearch()" type="text" <% if(setting.data.search_placehoder) {%> placeholder="<%= setting.data.search_placehoder %>" <%}%> name="search">
                        <% } %>   
                        <% if(setting.templateSetting.search_input_style == 2) { %>
                            <div style="position:relative;">
                                <svg id="micro-scope" style="position: absolute; left: 0; top: 50%; transform:translateY(-50%); margin-left: 18px" height="22" width="22" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512"><path d="M416 208c0 45.9-14.9 88.3-40 122.7L502.6 457.4c12.5 12.5 12.5 32.8 0 45.3s-32.8 12.5-45.3 0L330.7 376c-34.4 25.2-76.8 40-122.7 40C93.1 416 0 322.9 0 208S93.1 0 208 0S416 93.1 416 208zM208 352c79.5 0 144-64.5 144-144s-64.5-144-144-144S64 128.5 64 208s64.5 144 144 144z"/></svg>
                                <input class="faq-search-input faq-search-input-style2" style="color: <%= setting.templateSetting.placeholder_color %>" id="search-text" onkeyup="simpleSearch()" type="text" <% if(setting.data.search_placehoder) {%> placeholder="<%= setting.data.search_placehoder %>" <%}%> name="search">
                            </div>
                        <% } %>   
                    </div>
                <%}
                else{%>
                    <div class="faq-search-no-title">
                        <% if(setting.templateSetting.search_input_style == 1) { %>
                            <input class="faq-search-input faq-search-input-style1" style="color: <%= setting.templateSetting.placeholder_color %>" id="search-text" onkeyup="simpleSearch()" type="text" <% if(setting.data.search_placehoder) {%> placeholder="<%= setting.data.search_placehoder %>" <%}%> name="search">
                        <% } %>
                        <% if(setting.templateSetting.search_input_style == 2) { %>
                            <div style="position:relative;">
                                <svg id="micro-scope" style="position: absolute; left: 0; top: 50%; transform:translateY(-50%); margin-left: 18px" height="22" width="22" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512"><path d="M416 208c0 45.9-14.9 88.3-40 122.7L502.6 457.4c12.5 12.5 12.5 32.8 0 45.3s-32.8 12.5-45.3 0L330.7 376c-34.4 25.2-76.8 40-122.7 40C93.1 416 0 322.9 0 208S93.1 0 208 0S416 93.1 416 208zM208 352c79.5 0 144-64.5 144-144s-64.5-144-144-144S64 128.5 64 208s64.5 144 144 144z"/></svg>
                                <input class="faq-search-input faq-search-input-style2" style="color: <%= setting.templateSetting.placeholder_color %>" id="search-text" onkeyup="simpleSearch()" type="text" <% if(setting.data.search_placehoder) {%> placeholder="<%= setting.data.search_placehoder %>" <%}%> name="search">                                
                            </div>
                        <% } %>
                    </div>
                <%} %>
            <% } %>
        </div>
        <div>
            <% if(setting.data.show_intro_text && setting.data.intro_text_content) {%>
                <div class="faq-intro-text">
                    <span><%- setting.data.intro_text_content %></span>
                </div>
            <% } %>    
        </div>
        <div>
            <% if(!setting.data.dont_category_faq) {%>
                <div class="category-bar">
                    <% for(var i = 0; i < faqs.categories.length; i++) { %>
                        <div class="category-bar-item" id="faq-category<%- i %>" onclick="changeCategory('<%- i %>', '<%= setting.templateSetting.faq_hover_color%>', '<%= setting.templateSetting.category_bar_background %>', '<%= setting.templateSetting.banner_visible %>')"
                        <% if(i === 0){%> style="color: <%= setting.templateSetting.category_bar_color %>;background-color: <%= setting.templateSetting.category_bar_background %>; text-transform: <%=  setting.templateSetting.category_text_style %>"<%}
                        else{%>style="font-size: 16px !important; user-select: none; text-transform: <%=  setting.templateSetting.category_text_style %>; color: <%=  setting.templateSetting.category_bar_color %>"<%}%>>
                            <span class="category-bar-title">
                            <%- faqs.categories[i].title %>                                                  
                            </span>
                        </div>
                    <% } %>                                
                </div>
            <% } %>        
        </div>
    </div>
    <div class="faq-main">
        <% if(!setting.data.show_page_construction){%>
            <% if(faqs && !setting.data.dont_category_faq){ %>
                <div class="faq-main-container">
                    <div class="faq-category-container">
                        <% if(faqs.categories.length){%>
                            <div class="faq-container">
                                <div class="category-faq-container">
                                    <% for(var i=0; i < faqs.categories.length; i++) { %>
                                        <div <% if(i === 0){%> style="display: block"<%}
                                            else{%>style="display: none"<%}%> id="category<%- i %>">
                                            <div id="<%- faqs.categories[i].title %>" class="faq-category-title"><%- faqs.categories[i].title %></div>
                                            <% for(var j = 0; j < faqs.faq.length; j++) { %>
                                                <% if(faqs.faq[j].category_identify === faqs.categories[i].identify) {%>
                                                    <div class="faq-main-content">
                                                        <div class="faq-question-title" onclick="showHideContent(this)">
                                                            <i class="faq-icon fa-solid fa-plus" class="fa-solid fa-plus" <% if(j === 0){%> style="opacity: 0"<%}
                                                                else{%>style="opacity: 1"<%}%>></i>
                                                            <i class="fa-solid fa-minus faq-icon" <% if(j === 0){%> style="opacity: 1"<%}
                                                                else{%>style="opacity: 0"<%}%>></i>
                                                            <span class="listed-faq" value><%= faqs.faq[j].title %> </span>
                                                        </div>
                                                        <div id="faq-answer-content" class="faq-answer-content" <% if(j === 0){%> style="display: block"<%}
                                                            else{%>style="display: none"<%}%>>
                                                            <%- faqs.faq[j].content %> 
                                                        </div>
                                                    </div>
                                                <% } %>
                                            <%}%>
                                        </div>
                                    <% } %>
                                </div>
                            </div>
                        <% } %>
                    </div>
                </div>
            <% }
            else{ %>
                <div class="faq-dont-category">
                    <div style="display: flex; flex-direction: column">
                        <% for(var j = 0; j < faqs.faq.length; j++) { %>
                            <div class="faq-main-content">
                                <div class="faq-question-title" onclick="showHideContent(this)">
                                    <i class="faq-icon fa-solid fa-plus" class="fa-solid fa-plus" <% if(j === 0){%> style="opacity: 0"<%}
                                        else{%>style="opacity: 1"<%}%>></i>
                                    <i class="fa-solid fa-minus faq-icon" <% if(j === 0){%> style="opacity: 1"<%}
                                        else{%>style="opacity: 0"<%}%>></i>
                                    <span class="listed-faq"><%= faqs.faq[j].title %> </span>
                                </div>
                                <div class="faq-answer-content" <% if(j === 0){%> style="display: block"<%}
                                    else{%>style="display: none"<%}%>>
                                    <%- faqs.faq[j].content %> 
                                </div>
                            </div>
                        <%}%>
                    </div>
                </div>
            <% } %>
            <div class="category-faq-search">
                <div style="display: flex; flex-direction: column">
                    <% for(var i=0; i < faqs.categories.length; i++) { %>
                        <div>
                            <% for(var j=0; j < faqs.faq.length; j++) { %>
                                <% if(faqs.faq[j].category_identify === faqs.categories[i].identify) {%>
                                    <div class="faq-main-content">
                                        <div class="faq-question-title" onclick="showHideContent(this)">
                                            <i class="faq-icon fa-solid fa-plus" class="fa-solid fa-plus" style="opacity: 1"></i>
                                            <i class="fa-solid fa-minus faq-icon" style="opacity: 0;"></i>
                                            <span class="listed-faq faq-question-search"><%= faqs.faq[j].title %></span>
                                        </div>
                                        <div class="faq-answer-content faq-answer-search" style="display: none;">
                                            <%- faqs.faq[j].content %>
                                        </div>
                                    </div>
                                <% } %>
                            <%}%>
                        </div>
                    <% } %>
                </div>
            </div>
        <% }
        else{ %>
            <div class="page-under-contruction">
                <%- setting.data.page_under_contruction %>
            </div>
        <% } %>
        <div id="not-found-text">
            <% if(setting.data.search_not_found) {%>
                <%- setting.data.search_not_found %>
            <% } %>
        </div>
        <% if(setting.data.show_footer_text) {%>
            <div class="faq-footer-text">
                <% if(setting.data.footer_text_content) {%>
                    <%- setting.data.footer_text_content %>
                <% } %>
            </div>
        <% } %>
        <% if(setting.data.yanet_logo_visible) {%>
            <div class="" style="text-align: center; padding-bottom: 10px">
                <a href="https://apps.shopify.com/yanet-professional-faq-page" class="yanet-logo" target="_blank">Powered by Yanet</a>
            </div>
        <% } %>
        <% if(setting.templateSetting.btn_top_visible) {%>
            <div class="btn-back-to-top" onclick="topFunction()" id="back-to-top" title="Go to top">
                <i class="fa-solid fa-angles-up" style="color: #ffffff"></i>
            </div>
        <% } %>
    </div>
</div>
<script>
    let mybutton = document.getElementById("back-to-top");
    window.onscroll = function() {scrollFunction()};
    function scrollFunction() {
        if (document.body.scrollTop > 600 || document.documentElement.scrollTop > 600) {
            mybutton.style.display = "flex";
        } 
        else {
            mybutton.style.display = "none";
        }
    }
    function topFunction() {
        document.body.scrollIntoView({
            behavior: "smooth"
        });    
    }
    var categoryId = []
    function changeCategory(id, faq_hover_color, category_bar_background, banner_visible){
        if(!categoryId.some(item => {
            return item === id
        })){            
            categoryId.push(id)
        }
        var categoryShow = categoryId.filter(item => {
            return item === id
        })
        var categoryHide = categoryId.filter(item => {
            return item !== id
        })
        if(id != '0'){
            if(banner_visible){
                document.getElementById('faq-category0').style.backgroundColor = 'unset'
            }
            else{
                document.getElementById('faq-category0').style.backgroundColor = `${faq_hover_color}`
            }
            document.getElementById(`category0`).style.display = 'none'
        }
        else{
            document.getElementById('faq-category0').style.backgroundColor = `${category_bar_background}`
        }
        if(categoryId.length === 1){
            if(banner_visible){
                document.getElementById('faq-category0').style.backgroundColor = 'unset'
            }
            else{
                document.getElementById('faq-category0').style.backgroundColor = `${faq_hover_color}`
            }
        }
        for(i = 0; i < document.getElementsByClassName('faq-answer-content').length; i++){
            if(i === 0){
                document.getElementsByClassName('faq-answer-content')[0].style.display = 'block'
                document.getElementsByClassName('fa-plus')[0].style.opacity = '0'
                document.getElementsByClassName('fa-minus')[0].style.opacity = '1'
            }
            else{
                document.getElementsByClassName('faq-answer-content')[i].style.display = 'none'
                document.getElementsByClassName('fa-plus')[i].style.opacity = '1'
                document.getElementsByClassName('fa-minus')[i].style.opacity = '0'
            }
        }
        for(let i = 0; i < categoryId.length; i++){
            if(categoryId[i] == id){
                document.getElementById(`category${id}`).style.display = 'block'
                document.getElementById(`faq-category${categoryId[i]}`).style.backgroundColor = `${category_bar_background}`
            }
            else{
                document.getElementById(`category${categoryId[i]}`).style.display = 'none'
                if(banner_visible){
                    document.getElementById(`faq-category${categoryId[i]}`).style.backgroundColor = 'unset'
                }
                else{
                    document.getElementById(`faq-category${categoryId[i]}`).style.backgroundColor = `${faq_hover_color}`
                }
            }
        }
    }
    function showHideContent(el) {
        var content = el.nextElementSibling;
        var iconPlus = el.getElementsByClassName('fa-plus')
        var iconMinus = el.getElementsByClassName('fa-minus')
        if (content.style.display == "none") {
            content.style.display = "block";
        } 
        else {
            content.style.display = "none";
        }
        if (iconPlus[0].style.opacity === "0") {
            iconMinus[0].style.transform = "rotate(0deg)";
            iconMinus[0].style.transition  = "all 0s";
            iconMinus[0].style.opacity = "0";
            iconPlus[0].style.transform = "rotate(180deg)";
            iconPlus[0].style.opacity = "1";
            iconPlus[0].style.transition  = "all .7s";
        } 
        else {
            iconPlus[0].style.transform = "rotate(360deg)";
            iconPlus[0].style.transition  = "all 0s";
            iconPlus[0].style.opacity = "0";
            iconMinus[0].style.transform = "rotate(180deg)";
            iconMinus[0].style.transition  = "ease-in .3s";
            iconMinus[0].style.opacity = "1";
        }
    }
    function simpleSearch() {
        var input = document.getElementById("search-text");
        var filter = input.value.toLowerCase();
        var filterByQuestion = document.getElementsByClassName('faq-question-search');
        var filterByAnswer = document.getElementsByClassName('faq-answer-search');
        var nodeCategory = document.getElementsByClassName('category-faq-search');
        if(<%=  setting.data.dont_category_faq%> == false){
            if(document.getElementById('search-text').value === ''){
                document.getElementsByClassName("faq-main")[0].style.alignItems = "unset"
                document.getElementsByClassName("category-faq-container")[0].style.display = "block"
                document.getElementsByClassName('category-bar')[0].style.display = "flex"
                nodeCategory[0].style.display = "none"
                for(i = 0; i < document.getElementsByClassName("faq-category-search").length; i++) {
                    document.getElementsByClassName("faq-category-search")[i].style.display = "block"
                }
            }
            else{
                document.getElementsByClassName("faq-main")[0].style.alignItems = "center"
                document.getElementsByClassName("category-faq-container")[0].style.display = "none"
                document.getElementsByClassName('category-bar')[0].style.display = "none"
                nodeCategory[0].style.display = "flex"
                for(i = 0; i < document.getElementsByClassName("faq-category-search").length; i++) {
                    document.getElementsByClassName("faq-category-search")[i].style.display = "none"
                }
            }
            var checkexist = true;
            for (i = 0; i < filterByQuestion.length; i++) {
                if (filterByQuestion[i].innerText.toLowerCase().includes(filter)) {
                    filterByQuestion[i].parentNode.parentNode.style.display = "block";
                    checkexist = false;
                } 
                else {
                    for(j = 0; j < filterByAnswer.length; j++){
                        if(filterByAnswer[j].innerText.toLowerCase().includes(filter)){
                            filterByQuestion[j].parentNode.parentNode.style.display = "block";
                            checkexist = false;        
                        }
                        else{
                            filterByQuestion[i].parentNode.parentNode.style.display = "none";
                        }
                    }
                }
            }
            if (checkexist) {
                document.getElementById("not-found-text").style.display = "block";
                nodeCategory[0].style.display = "none";
            } else {
                document.getElementById("not-found-text").style.display = "none";
            }
        }
        else{
            if(document.getElementById('search-text').value == ''){
                document.getElementsByClassName('faq-dont-category')[0].style.display = "flex"

                nodeCategory[0].style.display = "none"
                for(i = 0; i < document.getElementsByClassName("faq-category-search").length; i++) {
                    document.getElementsByClassName("faq-category-search")[i].style.display = "block"
                }
            }
            else{
                document.getElementsByClassName('faq-dont-category')[0].style.display = "none"

                nodeCategory[0].style.display = "flex"
                for(i = 0; i < document.getElementsByClassName("faq-category-search").length; i++) {
                    document.getElementsByClassName("faq-category-search")[i].style.display = "none"
                }
            }
            var checkexist = true;
            for (i = 0; i < filterByQuestion.length; i++) {
                if (filterByQuestion[i].innerText.toLowerCase().includes(filter)) {
                    filterByQuestion[i].parentNode.parentNode.style.display = "flex";
                    checkexist = false;
                } 
                else {
                    for(j = 0; j < filterByAnswer.length; j++){
                        if(filterByAnswer[j].innerText.toLowerCase().includes(filter)){
                            filterByQuestion[j].parentNode.parentNode.style.display = "flex";
                            checkexist = false;        
                        }
                        else{
                            filterByQuestion[i].parentNode.parentNode.style.display = "none";
                        }
                    }
                }
            }
            if (checkexist) {
                document.getElementById("not-found-text").style.display = "block";
                nodeCategory[0].style.display = "none";
            } else {
                document.getElementById("not-found-text").style.display = "none";
            }        
        }
    }
</script>
